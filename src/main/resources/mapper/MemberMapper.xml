<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.esgproject.daaang_univ.dao.MemberDAO">

    <!-- 멤버 메인 리스트 -->
    <select id="memberList" resultType="com.esgproject.daaang_univ.dto.MemberDTO">
        <![CDATA[
        SELECT * FROM MEMBER
        ]]>
    </select>
    <!-- 멤버 삽입 -->
    <insert id="insertMember" parameterType="com.esgproject.daaang_univ.dto.MemberDTO">
        <![CDATA[
        INSERT INTO MEMBER (
            userid, userpwd,username,email,tel,joindate
        ) VALUES (
            #{userid}, #{userpwd}, #{username},#{email},#{tel}, now()
        )
        ]]>
    </insert>
</mapper>
